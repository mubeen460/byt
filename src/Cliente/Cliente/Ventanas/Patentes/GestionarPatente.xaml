<Page x:Class="Trascend.Bolet.Cliente.Ventanas.Patentes.GestionarPatente"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:custom="clr-namespace:Trascend.Bolet.ControlesByT;assembly=ControlesByT"
      mc:Ignorable="d" 
      xmlns:conv="clr-namespace:Trascend.Bolet.Cliente.Transformadores"
      xmlns:reglas="clr-namespace:Trascend.Bolet.Cliente.Validaciones"
      d:DesignHeight="550" d:DesignWidth="800"
      xmlns:recurso="clr-namespace:Trascend.Bolet.Cliente.Recursos"
	Title="{x:Static recurso:Etiquetas.titleAgregarAgente}" Loaded="Page_Loaded"
      KeepAlive="True">
    <Page.Resources>
        <conv:EnteroMinimoABlanco x:Key="convertir"></conv:EnteroMinimoABlanco>
        <conv:DosValresAUno x:Key="convertirADosValores"></conv:DosValresAUno>
        <Style x:Key="ElementInError" TargetType="{x:Type FrameworkElement}">
            <Style.Triggers>
                <Trigger Property="Validation.HasError" Value="True">
                    <Setter Property="ToolTip"
                    Value="{Binding (Validation.Errors)[0].ErrorContent, RelativeSource={x:Static RelativeSource.Self}}"/>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style TargetType="{x:Type DatePickerTextBox}">
            <Setter Property="Text" Value="{x:Static recurso:Etiquetas.dpkText}" />
        </Style>
    </Page.Resources>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="50"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="10"/>
        </Grid.RowDefinitions>


        <RadioButton Grid.Column="1" Name="_radioNacional" IsChecked="True" GroupName="GrupoTipoPatente" VerticalAlignment="Center">Patente Nacional</RadioButton>
        <RadioButton Grid.Column="1" Name="_radioExtranjero" GroupName="GrupoTipoPatente" Margin="120,0,0,0" VerticalAlignment="Center">Patente Extranjero</RadioButton>


        <ScrollViewer VerticalScrollBarVisibility="Auto" Grid.Column="1" Grid.Row="1">

            <TabControl Grid.Column="1" Grid.Row="1" Name="_tbcPestanas" DataContext="{Binding}">
                <TabItem Header="{x:Static recurso:Etiquetas.tabSolicitud}" Name="_tabSolicitud">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0"/>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Label Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblPatente}"/>
                        </Label>
                        <custom:ByTTextBox  Grid.Column="1" Grid.Row="1" Name="_txtIdSolicitud" VerticalAlignment="Center"  
                          MaxLength="255" Margin="0,0,5,0" IsReadOnly="True"
                             ToolTip="{Binding ElementName=_txtIdDatos, Path=(Validation.Errors)[0].ErrorContent}">
                            <custom:ByTTextBox.Text>
                                <Binding Path="Id" UpdateSourceTrigger="PropertyChanged">
                                    <Binding.ValidationRules>
                                        <reglas:Obligatorio ValidatesOnTargetUpdated="True" />
                                    </Binding.ValidationRules>
                                </Binding>
                            </custom:ByTTextBox.Text>
                        </custom:ByTTextBox>

                        <custom:ByTTextBox  Grid.Column="2" Grid.Row="1" Name="_txtDescripcionSolicitud" VerticalAlignment="Center"  
                            Grid.ColumnSpan="9" MaxLength="255" IsReadOnly="True"
                             ToolTip="{Binding ElementName=_txtDescripcionDatos, Path=(Validation.Errors)[0].ErrorContent}">
                            <custom:ByTTextBox.Text>
                                <Binding Path="Descripcion" UpdateSourceTrigger="PropertyChanged">
                                    <Binding.ValidationRules>
                                        <reglas:Obligatorio ValidatesOnTargetUpdated="True" />
                                    </Binding.ValidationRules>
                                </Binding>
                            </custom:ByTTextBox.Text>
                        </custom:ByTTextBox>

                        <Label Visibility="Collapsed" Grid.Column="0" Grid.Row="2" VerticalAlignment="Center" HorizontalContentAlignment="Right" Name="_lblAsociado">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblAsociado}"/>
                        </Label>
                        <Button Grid.Column="0" Grid.Row="2" Name="_btnIrAsociados" IsDefault="False" Height="25" HorizontalAlignment="Right" Click="_btnIrAsociados_Click">
                            <TextBlock Text="{x:Static recurso:Etiquetas.btnAsociado}"/>
                        </Button>

                        <Grid Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="7">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <custom:ByTTextBox IsReadOnly="True" Grid.Column="0" Grid.Row="1" Name="_txtIdAsociadoSolicitud" VerticalAlignment="Center" BorderBrush="LightGreen"
                         MaxLength="255" Margin="0,0,5,0"  MouseDoubleClick="_txtAsociadoSolicitud_MouseDoubleClick" Text="{Binding Path=Asociado.Id}"></custom:ByTTextBox>

                            <custom:ByTTextBox  MaxLength="255" Grid.Column="1" Grid.Row="1" Name="_txtAsociadoSolicitud" VerticalAlignment="Center" BorderBrush="LightGreen" 
                         Grid.ColumnSpan="6" MouseDoubleClick="_txtAsociadoSolicitud_MouseDoubleClick" IsReadOnly="True" Text="{Binding Path=Asociado.Nombre}"></custom:ByTTextBox>

                            <Label Name="_lblIdAsociadoSolicitud" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Right" VerticalAlignment="Center" Visibility="Collapsed">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblId}"/>
                            </Label>
                            <custom:ByTTextBox IsEnabled="True"  Grid.Column="1" Grid.Row="0" Name="_txtIdAsociadoSolicitudFiltrar" VerticalAlignment="Center" 
                         MaxLength="255" Grid.ColumnSpan="2" Visibility="Collapsed"  TamanoMaximo="8"></custom:ByTTextBox>

                            <Label Name="_lblNombreAsociadoSolicitud" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Right" VerticalAlignment="Center" Visibility="Collapsed">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblNombre}"/>
                            </Label>
                            <custom:ByTTextBox     Grid.Column="4" Grid.Row="0" Name="_txtNombreAsociadoSolicitud" VerticalAlignment="Center"  Grid.ColumnSpan="2"
                         MaxLength="255" Visibility="Collapsed"></custom:ByTTextBox>

                            <Button  Name="_btnConsultarAsociadoSolicitud" Width="65" IsDefault="False" Grid.Row="0" Grid.Column="7" Height="25" HorizontalAlignment="Right" Click="_btnConsultarAsociadoSolicitud_Click" Visibility="Collapsed">
                                <TextBlock Text="{x:Static recurso:Etiquetas.btnConsultar}"/>
                            </Button>
                            <ListView Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="7" Grid.RowSpan="2" Name="_lstAsociadosSolicitud" ItemsSource="{Binding}" 
                      SelectionMode="Single" MouseDoubleClick="_lstAsociadosSolicitud_MouseDoubleClick"
                      Style="{DynamicResource EstiloListView}" Visibility="Collapsed" Height="150"  VerticalAlignment="Top">
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn Width="45">
                                            <GridViewColumnHeader Click="_OrdenarAsociadoSolicitud_Click" Tag="Id" Content="{x:Static recurso:Etiquetas.lsvId}"/>
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock>
                                                    <TextBlock.Text>
                                                        <Binding Path="Id" Converter="{StaticResource convertir}"/>
                                                    </TextBlock.Text>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn DisplayMemberBinding="{Binding Path=Nombre}" Width="300">
                                            <GridViewColumnHeader Click="_OrdenarAsociadoSolicitud_Click" Tag="Nombre" Content="{x:Static recurso:Etiquetas.lsvNombre}"/>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </Grid>

                        <Label Visibility="Collapsed" Grid.Column="0" Grid.Row="3" VerticalAlignment="Center" HorizontalContentAlignment="Right" Name="_lblInteresado">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblInteresado}"/>
                        </Label>
                        <Button Grid.Column="0" Grid.Row="3" Name="_btnIrInteresados" IsDefault="False" Height="25" HorizontalAlignment="Right" Click="_btnIrInteresados_Click">
                            <TextBlock Text="{x:Static recurso:Etiquetas.btnInteresado}"/>
                        </Button>

                        <Grid Grid.Column="1" Grid.Row="3" Grid.ColumnSpan="7">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <custom:ByTTextBox IsReadOnly="True" Grid.Column="0" Grid.Row="1" Name="_txtIdInteresadoSolicitud" VerticalAlignment="Center" BorderBrush="LightGreen"
                         MaxLength="255" Margin="0,0,5,0"  MouseDoubleClick="_txtInteresadoSolicitud_MouseDoubleClick" Text="{Binding Path=Interesado.Id}"></custom:ByTTextBox>

                            <custom:ByTTextBox  MaxLength="255" Grid.Column="1" Grid.Row="1" Name="_txtInteresadoSolicitud" VerticalAlignment="Center" BorderBrush="LightGreen"
                         Grid.ColumnSpan="6" MouseDoubleClick="_txtInteresadoSolicitud_MouseDoubleClick" IsReadOnly="True" Text="{Binding Path=Interesado.Nombre}"></custom:ByTTextBox>

                            <Label Name="_lblIdInteresadoSolicitud" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Right" VerticalAlignment="Center" Visibility="Collapsed">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblId}"/>
                            </Label>
                            <custom:ByTTextBox IsEnabled="True"    Grid.Column="1" Grid.Row="0" Name="_txtIdInteresadoSolicitudFiltrar" VerticalAlignment="Center" 
                         MaxLength="255" Grid.ColumnSpan="2" Visibility="Collapsed"  TamanoMaximo="8"></custom:ByTTextBox>

                            <Label Name="_lblNombreInteresadoSolicitud" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Right" VerticalAlignment="Center" Visibility="Collapsed">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblNombre}"/>
                            </Label>
                            <custom:ByTTextBox     Grid.Column="4" Grid.Row="0" Name="_txtNombreInteresadoSolicitud" VerticalAlignment="Center"  Grid.ColumnSpan="2"
                         MaxLength="255" Visibility="Collapsed"></custom:ByTTextBox>

                            <Button  Name="_btnConsultarInteresadoSolicitud" Width="65" IsDefault="False" Grid.Row="0" Grid.Column="7" Height="25" HorizontalAlignment="Right" 
                                Click="_btnConsultarInteresadoSolicitud_Click" Visibility="Collapsed">
                                <TextBlock Text="{x:Static recurso:Etiquetas.btnConsultar}"/>
                            </Button>
                            <ListView Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="7" Grid.RowSpan="2" Name="_lstInteresadosSolicitud" ItemsSource="{Binding}" 
                      SelectionMode="Single" MouseDoubleClick="_lstInteresadosSolicitud_MouseDoubleClick"
                      Style="{DynamicResource EstiloListView}" Visibility="Collapsed" Height="150"  VerticalAlignment="Top">
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn Width="45">
                                            <GridViewColumnHeader Click="_OrdenarInteresadoSolicitud_Click" Tag="Id" Content="{x:Static recurso:Etiquetas.lsvId}"/>
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock>
                                                    <TextBlock.Text>
                                                        <Binding Path="Id" Converter="{StaticResource convertir}"/>
                                                    </TextBlock.Text>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn DisplayMemberBinding="{Binding Path=Nombre}" Width="300">
                                            <GridViewColumnHeader Click="_OrdenarInteresadoSolicitud_Click" Tag="Nombre" Content="{x:Static recurso:Etiquetas.lsvNombre}"/>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </Grid>

                        <custom:ByTTextBox  MaxLength="40" Grid.Column="8" Grid.Row="3" Name="_txtPaisSolicitud" VerticalAlignment="Center" 
                         Grid.ColumnSpan="2" Margin="3,0,0,0" Text="{Binding Path=Interesado.Ciudad}" IsReadOnly="True"></custom:ByTTextBox>

                        <custom:ByTTextBox  MaxLength="40" Grid.Column="10" Grid.Row="3" Name="_txtEstadoSolicitud" VerticalAlignment="Center" 
                         Grid.ColumnSpan="2" Margin="3,0,0,0" Text="{Binding Path=Interesado.Estado}" IsReadOnly="True"></custom:ByTTextBox>

                        <Separator Margin="10 0" Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="12" VerticalAlignment="Top"/>

                        <Grid Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="15" VerticalAlignment="Center">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>


                            <Label Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblPais}"/>
                            </Label>
                            <ComboBox  Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="1" Name="_cbxPaisIntSolicitud" Height="20" 
                              ItemsSource="{Binding}" DisplayMemberPath="NombreEspanol" SelectedIndex="0" SelectionChanged="_cbxPaisIntSolicitud_SelectionChanged">
                            </ComboBox>

                            <Label Grid.Column="6" Grid.Row="0" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblRefInteresado}"/>
                            </Label>
                            <custom:ByTTextBox Grid.Column="7" Grid.Row="0" Name="_txtReferenciaInteresado" VerticalAlignment="Center" 
                         Grid.ColumnSpan="1" Text="{Binding Path=ReferenciaInteresadoInternacional}"></custom:ByTTextBox>

                            <Label Grid.Column="8" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Grid.ColumnSpan="2">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblCodIntl}"/>
                            </Label>
                            <custom:ByTTextBox  MaxLength="100" Grid.Column="10" Grid.Row="0" Name="_txtCod" VerticalAlignment="Center" 
                         Margin="3,0,0,0" Text="{Binding Path=CodigoPatenteInternacional}"></custom:ByTTextBox>
                            <custom:ByTTextBox  MaxLength="100" Grid.Column="11" Grid.Row="0" Name="_txtNum" VerticalAlignment="Center" 
                         Margin="3,0,0,0" Text="{Binding Path=CorrelativoExpediente}"></custom:ByTTextBox>

                            <Label Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblAsocInt}"/>
                            </Label>
                            <custom:ByTTextBox  Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="7" Margin="3" Name="_txtAsociadoInternacionalSolicitud" VerticalAlignment="Center" MouseDoubleClick="_txtAsociadoInternacionalSolicitud_MouseDoubleClick" 
                          Text="" BorderBrush="LightGreen"></custom:ByTTextBox>

                            <!--Área de filtrado del asociado internacional-->


                            <Label Visibility="Collapsed" Name="_lblIdAsociadoIntFiltrar" Grid.Column="1" Grid.Row="1" VerticalAlignment="Top" HorizontalContentAlignment="Right">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblId}"/>
                            </Label>
                            <custom:ByTTextBox  Visibility="Collapsed"  Grid.Column="2" Grid.Row="1" Grid.ColumnSpan="1" Margin="3" Name="_txtIdAsociadoIntFiltrar" VerticalAlignment="Top"  
                          Text=""></custom:ByTTextBox>

                            <Label Visibility="Collapsed"  Name="_lblNombreAsociadoIntFiltrar" Grid.Column="3" Grid.Row="1" VerticalAlignment="Top" HorizontalContentAlignment="Right">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblNombre}"/>
                            </Label>
                            <custom:ByTTextBox  Visibility="Collapsed"  Grid.Column="4" Grid.Row="1" Grid.ColumnSpan="1" Margin="3" Name="_txtNombreAsociadoIntFiltrar" VerticalAlignment="Top"  
                          Text=""></custom:ByTTextBox>

                            <Button  Name="_btnConsultarAsociadoInternacionalSolicitud" Width="65" IsDefault="False"  Grid.Column="5" Grid.Row="1" Height="25" VerticalAlignment="Top" HorizontalAlignment="Right" Click="_btnConsultarAsociadoInternacionalSolicitud_Click" Visibility="Collapsed">
                                <TextBlock Text="{x:Static recurso:Etiquetas.btnConsultar}"/>
                            </Button>
                            <ListView Margin="0,30,0,0" Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="7" Grid.RowSpan="2" Name="_lstAsociadosInternacionalesSolicitud" ItemsSource="{Binding}" 
                      SelectionMode="Single" MouseDoubleClick="_lstAsociadosInternacionalesSolicitud_MouseDoubleClick"
                      Style="{DynamicResource EstiloListView}" Visibility="Collapsed" Height="150"  VerticalAlignment="Top">
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn Width="45">
                                            <GridViewColumnHeader Click="_OrdenarAsociadoSolicitud_Click" Tag="Id" Content="{x:Static recurso:Etiquetas.lsvId}"/>
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock>
                                                    <TextBlock.Text>
                                                        <Binding Path="Id" Converter="{StaticResource convertir}"/>
                                                    </TextBlock.Text>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn DisplayMemberBinding="{Binding Path=Nombre}" Width="300">
                                            <GridViewColumnHeader Click="_OrdenarAsociadoSolicitud_Click" Tag="Nombre" Content="{x:Static recurso:Etiquetas.lsvNombre}"/>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                            </ListView>



                            <!--Fin de Área de filtrado del asociado internacional-->
                            <Label Grid.Column="8" Grid.Row="1" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                                <TextBlock Text="{x:Static recurso:Etiquetas.lblReferencia}"/>
                            </Label>
                            <custom:ByTTextBox  MaxLength="70" Grid.Column="9" Grid.Row="1" Name="_txtReferenciaAsocInt" VerticalAlignment="Center" 
                         Grid.ColumnSpan="3" Text="{Binding Path=ReferenciaAsociadoInternacional}"></custom:ByTTextBox>
                        </Grid>

                        <Separator Margin="10 0" Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="12" VerticalAlignment="Bottom"/>
                        <!--Cuarta Fila-->
                        <Label Grid.Column="0" Grid.Row="5" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblCaso}"/>
                        </Label>

                        <custom:ByTTextBox Grid.Column="1" Grid.ColumnSpan="5" Grid.Row="5" Name="_txtCasoSolicitud" VerticalAlignment="Center"
                         MaxLength="255" Margin="0,0,5,0"  Text="{Binding Path=PrimeraReferencia}"></custom:ByTTextBox>
                        <!--Fin cuarta fila-->

                        <!--Quinta Fila-->
                        <Label Grid.Column="0" Grid.Row="6" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblPrioridad}"/>
                        </Label>
                        <custom:ByTTextBox Grid.Column="1" Grid.Row="6" Grid.ColumnSpan="2"  Name="_txtPrioridadSolicitud" VerticalAlignment="Center"
                         MaxLength="255" Margin="0,0,5,0"  Text="{Binding Path=CPrioridad}"></custom:ByTTextBox>


                        <Label Grid.Column="3" Grid.Row="6" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblFecha}"/>
                        </Label>
                        <DatePicker  Grid.Column="4" Grid.Row="6" Name="_dpkFechaPrioridadSolicitud" Grid.ColumnSpan="2"
                                    Focusable="False" BorderBrush="{x:Null}" VerticalAlignment="Center" Margin="0,0,5,0">
                            <DatePicker.SelectedDate>
                                <Binding Path="FechaPrioridad" UpdateSourceTrigger="PropertyChanged"/>
                            </DatePicker.SelectedDate>
                        </DatePicker>

                        <Label Grid.Column="6" Grid.Row="6" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblPais}"/>
                        </Label>
                        <ComboBox  Grid.Column="7" Grid.ColumnSpan="3" Grid.Row="6" Name="_cbxPaisSolicitud" Height="20" 
                              ItemsSource="{Binding}" DisplayMemberPath="NombreEspanol" SelectedIndex="0" SelectionChanged="_cbxPaisSolicitud_SelectionChanged">
                        </ComboBox>
                        <!--Fin Quinta Fila-->

                        <!--Nuevo-->

                        <!--Sexta Fila-->
                        <Label Grid.Column="0" Grid.Row="7" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblOrden}"/>
                        </Label>
                        <custom:ByTTextBox Grid.Column="1" Grid.Row="7" Name="_txtOrdenSolicitud" VerticalAlignment="Center"
                         MaxLength="255" Margin="0,0,5,0"  Text="{Binding Path=Orden}"></custom:ByTTextBox>


                        <Label Grid.Column="2" Grid.Row="7" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblFecha}"/>
                        </Label>
                        <DatePicker  Grid.Column="3" Grid.Row="7" Name="_dpkFechaOrdenSolicitud" Grid.ColumnSpan="2"
                                    Focusable="False" BorderBrush="{x:Null}" VerticalAlignment="Center" Margin="0,0,5,0">
                            <DatePicker.SelectedDate>
                                <Binding Path="FechaOrden" UpdateSourceTrigger="PropertyChanged"/>
                            </DatePicker.SelectedDate>
                        </DatePicker>

                        <Label Grid.Column="5" Grid.Row="7" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblTipo}"/>
                        </Label>
                        <ComboBox  Grid.Column="6" Grid.ColumnSpan="2" Grid.Row="7" Name="_cbxTipoSolicitud" Height="20" 
                              ItemsSource="{Binding}" DisplayMemberPath="Descripcion" SelectedIndex="0" SelectionChanged="_cbxTipoPatenteSolicitud_SelectionChanged">
                        </ComboBox>
                        <!--Fin Sexta Fila-->

                        <!--Séptima Fila-->
                        <Button  Name="_btnDisenoSolicitud" HorizontalAlignment="Center" Grid.Column="0" Grid.Row="9" VerticalAlignment="Center" 
                           Width="90" Click="_btnDisenoSolicitud_Click">Diseño</Button>

                        <CheckBox  Margin="3" Grid.Column="2" Grid.ColumnSpan="3" Grid.Row="9" VerticalAlignment="Center" 
                              Name="_chkMemoriaTraducidaSolicitud" IsChecked="{Binding Path=BIme}">
                            <TextBlock Text="{x:Static recurso:Etiquetas.chkMemoriaTraducidaSolicitud}" />
                        </CheckBox>

                        <Label Grid.Column="5" Grid.Row="9" VerticalAlignment="Center" HorizontalContentAlignment="Right">
                            <TextBlock Text="{x:Static recurso:Etiquetas.lblPresentacion}"/>
                        </Label>
                        <ComboBox  Grid.Column="6" Grid.ColumnSpan="2" Grid.Row="9" Name="_cbxPresentacionSolicitud" Height="20" 
                              ItemsSource="{Binding}" DisplayMemberPath="Descripcion" SelectedIndex="0" SelectionChanged="_cbxPresentacionSolicitud_SelectionChanged">
                        </ComboBox>
                        <!--Fin Séptima Fila-->

                        <!--Octava Fila-->
                        <Label Grid.Column="0" Grid.Row="10" VerticalAlignment="Center" HorizontalContentAlignment="Right">
