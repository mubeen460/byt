<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Trascend.Bolet.ObjetosComunes"
                   namespace="Trascend.Bolet.ObjetosComunes.Entidades">

  <class name="EmailAsociado" table="FAC_ASOCIADO_EMAILS">
    
    <id name="Id" column="CODIGO" type="string"/>
    
    <property name="Email" column="XEMAIL" type="string"/>


    <many-to-one name="Asociado" class="Asociado">
      <column name="CASOCIADO"/>
    </many-to-one>
    
    <many-to-one name="TipoEmailAsociado" class="TipoEmailAsociado">
      <column name="CEMAIL"/>
    </many-to-one>
    
  </class>
</hibernate-mapping>



