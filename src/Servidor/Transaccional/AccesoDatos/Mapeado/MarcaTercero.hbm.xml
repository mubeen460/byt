<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Trascend.Bolet.ObjetosComunes"
                   namespace="Trascend.Bolet.ObjetosComunes.Entidades">

  <class name="MarcaTercero" table="MYP_MTERCEROS">
    <composite-id>
      <key-property name="Id" column="`NSECUEN`" />
      <key-many-to-one name="Fusion" column="CFUSION" class="Fusion"/>
    </composite-id>

    <property name="Nombre" type="String" column="`XNOMBRE`"/>

    <many-to-one name="Pais" class="Pais">
      <column name="CPAIS"/>
    </many-to-one>

    <many-to-one name="Estado" class="Estado">
      <column name="CESTADO"/>
    </many-to-one>
    
    <many-to-one name="Nacionalidad" class="Pais">
      <column name="CNACIONALIDAD"/>
    </many-to-one>
    
  </class>
</hibernate-mapping>


